{
  "manifest_version": 2,
  "name": "Audio switcher",
  "version": "0.2",

  "description": "Audio switcher",

  "icons": {
   "48": "icons/icon-48.png"
  },
   "permissions": [
		"activeTab"
  ],

   "browser_action": {
    "default_icon": {
      "128": "icons/icon-128.png",
      "48": "icons/icon-48.png",
      "20": "icons/icon-20.png"
    },
    "default_title": "Audio switcher",
    "default_popup": "popup.html"
  }

}